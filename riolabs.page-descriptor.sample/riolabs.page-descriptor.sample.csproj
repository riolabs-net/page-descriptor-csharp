    <RootNamespace>riolabs.common.frontend_descriptor.sample</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <OutputType>Exe</OutputType>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\riolabs.page-descriptor.bs\riolabs.page-descriptor.bs.csproj" />
